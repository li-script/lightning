const validate_random = |x,y| {
   if y {
      const r = math.random(x,y)
      assert(x <= r && r <= y)
   } else if x {
      const r = math.random(x)
      assert(0 <= r && r <= x)
   } else {
      const r = math.random()
      assert(0 <= r && r <= 1)
   }
}


for i in 0..100 {
   validate_random(0,100)
   validate_random(100)
   validate_random()
}